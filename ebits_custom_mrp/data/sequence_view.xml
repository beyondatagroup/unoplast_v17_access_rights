<odoo>
    <data noupdate="1">
        <record id="sf_manufacturing_order_sequence" model="ir.sequence">
            <field name="name">SF Manufacturing Orders Sequence</field>
            <field name="code">sf.manufacturing.order.code</field>
            <field eval="3" name="padding"/>
            <field name="prefix">SEQ/</field>
        </record>
        
        <record id="sf_manufacturing_order_unbuild_sequence" model="ir.sequence">
            <field name="name">SF Manufacturing Orders Unbuild Sequence</field>
            <field name="code">sf.manufacturing.order.unbuild.code</field>
            <field eval="3" name="padding"/>
            <field name="prefix">UB/</field>
        </record>
    </data>
</odoo>
