        <tr style="Table2"><para style="G1">[[repeatIn(o.lines,'line')]]</para>
            <td> [[ line.product_id.type == 'service' and removeParentNode('tr') or '']]
                <para style="F1"><seq id="seq1"/></para>
            </td>
            <td>
                <para style="F1"><b>[[ line.product_id.name_get()[0][1] ]]</b></para>
            </td>
            <td>
                <para style="G1">[[ formatLang(line.qty, digits=3) ]]</para>
            </td>
            <td>
                <para style="H1">[[ line.product_id.uom_id.name ]]</para>
            </td>
            <td>
                <para style="G1">[[ formatLang(line.price_unit) ]]</para>
            </td>
            <td>
                <para style="G1">[[ formatLang(line.discount) ]]</para>
            </td>
            <td>
                <para style="H1">[[', '.join([(lt.description or '') for lt in line.tax_ids]) ]]</para>
            </td>
            <td>
                <para style="G1">[[ formatLang(line.price_subtotal) ]]</para>
            </td>
        </tr>
    </blockTable>
    <para style="F1"><font color="white">.</font></para>
    <blockTable colWidths="540.0" style="Table1">
